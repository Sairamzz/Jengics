<?xml version="1.0"?>
<robot name="jenga_block">
    <!-- Define Materials -->
    <material name="white">
        <color rgba="1 1 1 1"/>
    </material>
    <material name="red">
        <color rgba="1 0 0 1"/>
    </material>
    <material name="blue">
        <color rgba="0.2 0.2 1 1"/>
    </material>
    <material name="black">
        <color rgba="0 0 0 1"/>
    </material>

    <link name="jenga_block">
        <visual>
            <origin xyz="0 0 0" />
            <geometry>
                <box size="0.075 0.025 0.015"/>
            </geometry>
            <material name="blue" />
        </visual>

        <collision>
            <origin xyz="0 0 0" />
            <geometry>
                <box size="0.075 0.025 0.015"/>
            </geometry>
        </collision>

        <inertial>
            <origin xyz="0 0 0" />
            <mass value="0.002"/>
            <inertia ixx="1.4166666666666668e-07" ixy="0" ixz="0"
                    iyy="9.75e-07" iyz="0"
                    izz="1.0416666666666667e-06" />
        </inertial>
    </link>

    <!-- Gazebo physics properties -->
    <gazebo reference="jenga_block">
        <material>Gazebo/Blue
            <script>
                <uri>model://jenga_block/materials/scripts</uri>
                <name>Jenga/CatTexture</name>
            </script>
        </material>
        <gravity>true</gravity>
        <mu1>0.7</mu1>
        <mu2>0.7</mu2>
        <!-- <dampingFactor>1</dampingFactor> -->
        <kp>50000</kp> <!--contact stiffness -->
        <kd>2000</kd> <!-- contact damping -->
        

    </gazebo>

</robot>
